#include<iostream>
using namespace std;
int main()
{
    char c;
    int a,b;
    cin>>c;
    a=(c=='A'||c=='E'||c=='I'||c=='O'||c=='U');
    b=(c=='a'||c=='e'||c=='i'||c=='o'||c=='u');
    if(a||b)
    {
        cout<<c<<"vowel";
    }
    else if(!(a||b))
    {
        cout<<c<<"consonants";
    }
    else
    {
        cout<<c<<"invalid";
    }
    return 0;
}

